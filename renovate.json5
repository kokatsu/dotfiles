{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  labels: [
    'dependencies',
  ],
  minimumReleaseAge: '1 day',
  ignorePaths: [
    'nix/npm-locks/**',
  ],
  customDatasources: {
    "claude-code": {
      defaultRegistryUrlTemplate: "https://storage.googleapis.com/claude-code-dist-86c565f3-f756-42ad-8dfa-d59b1c096819/claude-code-releases/latest",
      format: "plain",
    },
  },
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/nix/overlays/default\\.nix$/',
      ],
      matchStrings: [
        '#\\s*Renovate:\\s*datasource=(?<datasource>[\\w.-]+)\\s+depName=(?<depName>[\\w@/-]+)\\n\\s+[\\w-]+\\s*=\\s*_final:\\s*prev:\\s*let\\n\\s+version\\s*=\\s*"(?<currentValue>[^"]+)"',
      ],
      versioningTemplate: 'semver',
    },
    {
      customType: 'regex',
      managerFilePatterns: [
        '/nix/overlays/default\\.nix$/',
      ],
      matchStrings: [
        '#\\s*Renovate:\\s*datasource=(?<datasource>[\\w.-]+)\\s+depName=(?<depName>[\\w@/-]+)\\n\\s+[\\w-]+\\s*=\\s*_final:\\s*prev:\\s*\\{\\n\\s+[\\w-]+\\s*=\\s*prev\\.[\\w.]+\\s+rec\\s*\\{\\n\\s+pname\\s*=\\s*"[^"]+";\\n\\s+version\\s*=\\s*"(?<currentValue>[^"]+)"',
      ],
      versioningTemplate: 'semver',
    },
  ],
  packageRules: [
    {
      matchFileNames: [
        'nix/overlays/default.nix',
      ],
      groupName: 'nix overlay packages',
      semanticCommitType: 'build',
      semanticCommitScope: 'deps',
    },
    {
      matchDatasources: [
        'github-releases',
      ],
      extractVersion: '^v?(?<version>.*)$',
    },
    {
      matchManagers: [
        'github-actions',
      ],
      minimumReleaseAge: '7 days',
      internalChecksFilter: 'strict',
    },
    {
      matchManagers: [
        'github-actions',
      ],
      matchUpdateTypes: [
        'digest',
      ],
      enabled: false,
    },
  ],
}
