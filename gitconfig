[alias]
a = add
au = add -u
cm = commit -m
co = checkout
cp = cherry-pick
d = diff
ds = diff --staged
f = fetch origin
fd = !git branch | fzf | xargs git checkout
l = log --date=format-local:'%Y/%m/%d %a %H:%M' --pretty=format:'%C(yellow)%h %C(bold blue)(%ad)%C(auto)%d %C(reset)%s %C(bold magenta)<%an>' --graph
p = pull
po = push origin HEAD
s = status
sw = switch

[fetch]
prune = true
